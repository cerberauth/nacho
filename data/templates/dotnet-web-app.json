{
  "$schema": "https://schemas.cerberauth.com/templates/draft/2024-09/application.schema.json",
  "name": ".NET Web App",
  "description": ".NET Web Application with OpenID Connect.",
  "identifier": "dotnet-web-app",
  "icon": {
    "contentUrl": "https://nacho.cerberauth.com/logos/dotnet.svg"
  },
  "technologies": [
    "c#",
    ".net"
  ],
  "client": {
    "applicationType": "webApplication",
    "grantTypes": [
      "authorization_code",
      "pkce",
      "refresh_token"
    ],
    "tokenEndpointAuthMethods": [
      "client_secret_basic",
      "client_secret_post"
    ],
    "scopes": [
      "openid",
      "offline_access",
      "profile",
      "email"
    ]
  }
}