{
  "$schema": "https://schemas.cerberauth.com/templates/draft/2024-09/application.schema.json",
  "name": "Dart SPA",
  "description": "Dart Single Page Application with OpenID Connect.",
  "identifier": "dart-spa",
  "icon": {
    "contentUrl": "https://nacho.cerberauth.com/logos/dart.svg"
  },
  "technologies": [
    "javascript",
    "dart"
  ],
  "libraries": [
    {
      "name": "openid_client",
      "description": "OpenID Connect client library for Dart browser application.",
      "url": "https://github.com/appsup-dart/openid_client"
    }
  ],
  "client": {
    "applicationType": "spa",
    "grantTypes": [
      "authorization_code",
      "pkce"
    ],
    "tokenEndpointAuthMethods": [
      "none"
    ],
    "scopes": [
      "openid",
      "profile",
      "email"
    ]
  }
}