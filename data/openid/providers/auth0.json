{
  "$schema": "https://schemas.cerberauth.com/openid/draft/2024-09/provider.schema.json",
  "name": "Auth0",
  "abstract": "Auth0 is an identity and access management platform that provides a comprehensive set of features to authenticate and authorize users.",
  "identifier": "auth0",
  "icon": {
    "contentUrl": "https://nacho.cerberauth.com/logos/auth0.svg"
  },
  "website": "https://auth0.com",
  "license": "Proprietary",
  "featureList": [
    {
      "identifier": "implicit_grant",
      "status": "supported",
      "url": "https://auth0.com/docs/flows/implicit-flow-with-form-post"
    },
    {
      "identifier": "authorization_code_grant",
      "status": "supported",
      "url": "https://auth0.com/docs/flows/authorization-code-flow"
    },
    {
      "identifier": "pkce_extension",
      "status": "supported",
      "url": "https://auth0.com/docs/get-started/authentication-and-authorization-flow/authorization-code-flow-with-pkce"
    },
    {
      "identifier": "jar_extension",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials#jwt-secured-authorization-request-jar-"
    },
    {
      "identifier": "client_credentials_grant",
      "status": "supported",
      "url": "https://auth0.com/docs/flows/client-credentials-flow"
    },
    {
      "identifier": "password_grant",
      "status": "supported",
      "url": "https://auth0.com/docs/flows/resource-owner-password-flow"
    },
    {
      "identifier": "refresh_token_grant",
      "status": "supported",
      "url": "https://auth0.com/docs/tokens/refresh-tokens/use-refresh-tokens"
    },
    {
      "identifier": "urn:ietf:params:oauth:grant-type:device_code",
      "status": "supported",
      "url": "https://auth0.com/docs/flows/device-authorization-flow"
    },
    {
      "identifier": "urn:ietf:params:oauth:grant-type:token-exchange",
      "status": "supported",
      "url": "https://auth0.com/docs/authenticate/custom-token-exchange"
    },
    {
      "identifier": "authorization_endpoint",
      "status": "supported",
      "url": "https://auth0.com/docs/api/authentication#authorize-endpoint"
    },
    {
      "identifier": "token_endpoint",
      "status": "supported",
      "url": "https://auth0.com/docs/api/authentication#-post-oauth-token-"
    },
    {
      "identifier": "userinfo_endpoint",
      "status": "supported",
      "url": "https://auth0.com/docs/api/authentication#get-user-info"
    },
    {
      "identifier": "token_revocation_endpoint",
      "status": "partial",
      "description": "Auth0 support only revocation of refresh tokens",
      "url": "https://auth0.com/docs/api/authentication#revoke-refresh-token"
    },
    {
      "identifier": "token_introspection_endpoint",
      "status": "not_supported"
    },
    {
      "identifier": "openid_connect_discovery_endpoint",
      "status": "supported",
      "url": "https://auth0.com/docs/get-started/applications/configure-applications-with-oidc-discovery"
    },
    {
      "identifier": "oauth_authorization_server_metadata_endpoint",
      "status": "partial",
      "description": "Supports through OpenID Connect Discovery",
      "url": "https://auth0.com/docs/get-started/applications/configure-applications-with-oidc-discovery"
    },
    {
      "identifier": "none_prompt",
      "status": "supported",
      "url": "https://auth0.com/docs/api/authentication#authorize"
    },
    {
      "identifier": "login_prompt",
      "status": "supported",
      "url": "https://auth0.com/docs/api/authentication#authorize"
    },
    {
      "identifier": "consent_prompt",
      "status": "not_supported"
    },
    {
      "identifier": "select_account_prompt",
      "status": "not_supported"
    },
    {
      "identifier": "create_prompt",
      "status": "not_supported"
    },
    {
      "identifier": "none_token_endpoint_auth",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "client_secret_basic_token_endpoint_auth",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "client_secret_post_token_endpoint_auth",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "client_secret_jwt_token_endpoint_auth",
      "status": "not_supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "private_key_jwt_token_endpoint_auth",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "tls_client_auth_token_endpoint_auth",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/application-credentials"
    },
    {
      "identifier": "dynamic_client_registration",
      "status": "supported",
      "url": "https://auth0.com/docs/get-started/applications/dynamic-client-registration"
    },
    {
      "identifier": "refresh_token_rotation",
      "status": "supported",
      "url": "https://auth0.com/docs/secure/tokens/refresh-tokens/refresh-token-rotation"
    }
  ]
}