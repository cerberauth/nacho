{
  "$schema": "https://schemas.cerberauth.com/templates/draft/2024-09/client.schema.json",
  "name": "Echo App",
  "description": "Echo Application with OpenID Connect.",
  "identifier": "golang-echo-app",
  "icon": {
    "contentUrl": "https://nacho.cerberauth.com/logos/golang-echo.png"
  },
  "technologies": [
    "go",
    "echo"
  ],
  "client": {
    "applicationType": "webApplication",
    "grantTypes": [
      "authorization_code",
      "pkce",
      "refresh_token"
    ],
    "tokenEndpointAuthMethods": [
      "client_secret_basic",
      "client_secret_post"
    ],
    "scopes": [
      "openid",
      "offline_access",
      "profile",
      "email"
    ]
  }
}