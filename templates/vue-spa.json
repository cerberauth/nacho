{
  "$schema": "https://schemas.cerberauth.com/templates/draft/2024-09/client.schema.json",
  "name": "Vue SPA",
  "description": "Vue Single Page Application with OpenID Connect.",
  "identifier": "vue-spa",
  "icon": {
    "contentUrl": "https://nacho.cerberauth.com/logos/vue.svg"
  },
  "technologies": [
    "javascript",
    "vue"
  ],
  "client": {
    "applicationType": "spa",
    "grantTypes": [
      "authorization_code",
      "pkce"
    ],
    "tokenEndpointAuthMethods": [
      "none"
    ],
    "scopes": [
      "openid",
      "profile",
      "email"
    ]
  }
}